-- Local Player


local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewSlider("WalkSpeed", "Speed of Walking", 350, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("JumpPower", "Power Of Jump", 500, 25, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlayerSection:NewButton("Reset WalkSpeed", "Reset Your Speed", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

PlayerSection:NewButton("Reset Jump Power", "Reset Your Jump Power", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)

PlayerSection:NewToggle("Infinite Jump", "Basically Fly", function(state)
    if state then
        loadstring(game:HttpGet('https://pastebin.com/raw/N3EqajFs'))()
    else
        loadstring(game:HttpGet('https://pastebin.com/raw/ZHMtrKfG'))()
    end
end)








